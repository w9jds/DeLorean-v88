{
    "extends": [
      "tslint-react"
    ],
    "rules": {
      "ban": false,
      "class-name": true,
      "comment-format": [
        true,
        "check-space"
      ],
      "eofline": false,
      "indent": [
        true,
        "spaces"
      ],
      "interface-name": [
        true,
        "never-prefix"
      ],
      "jsdoc-format": true,
      "jsx-no-lambda": false,
      "jsx-alignment": false,
      "jsx-no-multiline-js": false,
      "jsx-wrap-multiline": false,
      "jsx-boolean-value": false,
      "label-position": true,
      "no-any": false,
      "no-arg": true,
      "no-bitwise": false,
      "no-console": [
        true,
        "log",
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace"
      ],
      "no-consecutive-blank-lines": true,
      "no-construct": true,
      "no-debugger": true,
      "no-duplicate-variable": true,
      "no-empty": true,
      "no-eval": true,
      "no-string-literal": true,
      "no-switch-case-fall-through": true,
      "no-trailing-whitespace": false,
      "no-unused-expression": true,
      "one-line": [
        true,
        "check-catch",
        "check-open-brace",
        "check-whitespace"
      ],
      "quotemark": [
        true,
        "single",
        "jsx-double"
      ],
      "radix": true,
      "semicolon": [
        true,
        "always"
      ],
      "switch-default": true,
      "trailing-comma": false,
      "typedef": [
        true,
        "parameter",
        "property-declaration"
      ],
      "typedef-whitespace": [
        true,
        {
          "call-signature": "nospace",
          "index-signature": "nospace",
          "parameter": "nospace",
          "property-declaration": "nospace"
        }
      ],
      "variable-name": [
        true,
        "ban-keywords",
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-module",
        "check-operator",
        "check-separator",
        "check-type",
        "check-typecast"
      ]
    }
  }